@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix wl:      <http://www.wsmo.org/ns/wsmo-lite#> .
@prefix sawsdl:  <http://www.w3.org/ns/sawsdl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix msm:     <http://iserve.kmi.open.ac.uk/ns/msm#> .
@prefix dcterms:  <http://purl.org/dc/elements/1.1/> .
@prefix trust:  <http://www.compose-project.eu/ns/web-of-things/trust#> .
@prefix profile:  <http://www.compose-project.eu/ns/web-of-things/security/profiles#> .

<http://localhost/services/Weather_API_C>
      a       msm:Service ;
	  trust:hasProfile <http://localhost/services/Weather_API_C#TRUST_PROFILE>.

<http://localhost/services/Weather_API_C#TRUST_PROFILE> 
       a    trust:TrustProfile;
	  trust:hasAttribute <http://localhost/services/Weather_API_C#NFP_RATING> ;
	  trust:hasAttribute <http://localhost/services/Weather_API_C#NFP_AVAILABILITY> .
		
<http://localhost/services/Weather_API_C#NFP_RATING>
		a   wl:NonfunctionalParameter , trust:Rating ;
		trust:hasValue "http://www.compose-project.eu/ns/web-of-things/trust#twoStar"^^<http://www.compose-project.eu/ns/web-of-things/trust#RatingScale> .
		
<http://localhost/services/Weather_API_C#NFP_AVAILABILITY>
		a   wl:NonfunctionalParameter , trust:Availability ;
		trust:hasValue "0.6"^^<http://www.w3.org/2001/XMLSchema#double> .